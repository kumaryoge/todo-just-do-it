{
    "manifest_version": 3,
    "name": "Todo: Just do it",
    "description": "Todo: Smart task lists with due dates, projects and tags",
    "version": "1.6",
    "action": {
        "default_popup": "index.html",
        "default_title": "Todo: Just do it!"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
        "storage"
    ]
}
